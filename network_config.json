{
  "network": {
    "name": "MediVote Local Network",
    "network_id": "medivote_mainnet",
    "coordinator_port": 8083,
    "discovery_interval": 30,
    "node_timeout": 300,
    "max_nodes": 1000
  },
  "api": {
    "enabled": true,
    "port": 8083,
    "rate_limit": 100
  },
  "storage": {
    "data_dir": "./network_data",
    "backup_interval": 3600
  },
  "security": {
    "trusted_ips": ["127.0.0.1", "localhost"],
    "admin_ips": ["127.0.0.1", "localhost"]
  }
}